{
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "plugins": [],
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "env": {
    "node": true,
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "globals": {},
  "rules": {
    "semi": "error",
    "no-unused-vars": "warn",
    "prettier/prettier": [
      "error",
      {
        "singleQuote": false
      }
    ]
  }
}
